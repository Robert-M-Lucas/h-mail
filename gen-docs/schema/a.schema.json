{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Authorized",
  "oneOf": [
    {
      "type": "string",
      "enum": [
        "Unauthorized"
      ]
    },
    {
      "type": "object",
      "properties": {
        "Success": {
          "$ref": "#/$defs/SendEmailResponseAuthed"
        }
      },
      "additionalProperties": false,
      "required": [
        "Success"
      ]
    }
  ],
  "$defs": {
    "DeliverEmailResponse": {
      "oneOf": [
        {
          "type": "string",
          "enum": [
            "Success",
            "UserNotFound",
            "BadRequest",
            "SenderIpNotAuthed"
          ]
        },
        {
          "type": "object",
          "properties": {
            "DoesNotMeetPolicy": {
              "$ref": "#/$defs/PowPolicy"
            }
          },
          "additionalProperties": false,
          "required": [
            "DoesNotMeetPolicy"
          ]
        },
        {
          "type": "object",
          "properties": {
            "PowFailure": {
              "$ref": "#/$defs/PowFailureReason"
            }
          },
          "additionalProperties": false,
          "required": [
            "PowFailure"
          ]
        }
      ]
    },
    "PowFailureReason": {
      "oneOf": [
        {
          "type": "string",
          "enum": [
            "FailedNoRetry",
            "NotFoundCanRetry",
            "BadRequestCanRetry",
            "BadIPCanRetry"
          ]
        },
        {
          "type": "object",
          "properties": {
            "DoesNotMeetPolicyMinimum": {
              "type": "integer",
              "format": "uint32",
              "minimum": 0
            }
          },
          "additionalProperties": false,
          "required": [
            "DoesNotMeetPolicyMinimum"
          ]
        }
      ]
    },
    "PowPolicy": {
      "type": "object",
      "properties": {
        "accepted": {
          "type": "integer",
          "format": "uint32",
          "minimum": 0
        },
        "minimum": {
          "type": "integer",
          "format": "uint32",
          "minimum": 0
        },
        "personal": {
          "type": "integer",
          "format": "uint32",
          "minimum": 0
        }
      },
      "required": [
        "minimum",
        "accepted",
        "personal"
      ]
    },
    "SendEmailResponseAuthed": {
      "oneOf": [
        {
          "type": "string",
          "enum": [
            "SendingFailed"
          ]
        },
        {
          "type": "object",
          "properties": {
            "DeliverResponse": {
              "$ref": "#/$defs/DeliverEmailResponse"
            }
          },
          "additionalProperties": false,
          "required": [
            "DeliverResponse"
          ]
        }
      ]
    }
  }
}