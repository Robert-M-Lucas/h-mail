{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "CreateAccountResponse",
  "oneOf": [
    {
      "type": "string",
      "enum": [
        "Success",
        "BadUsername",
        "UsernameInUse",
        "BadPassword"
      ]
    },
    {
      "type": "object",
      "properties": {
        "DoesNotMeetPolicy": {
          "type": "integer",
          "format": "uint32",
          "minimum": 0
        }
      },
      "additionalProperties": false,
      "required": [
        "DoesNotMeetPolicy"
      ]
    },
    {
      "type": "object",
      "properties": {
        "PowFailure": {
          "$ref": "#/$defs/PowFailureReason"
        }
      },
      "additionalProperties": false,
      "required": [
        "PowFailure"
      ]
    }
  ],
  "$defs": {
    "PowFailureReason": {
      "oneOf": [
        {
          "type": "string",
          "enum": [
            "FailedNoRetry",
            "NotFoundCanRetry",
            "BadRequestCanRetry",
            "BadIPCanRetry"
          ]
        },
        {
          "type": "object",
          "properties": {
            "DoesNotMeetPolicyMinimum": {
              "type": "integer",
              "format": "uint32",
              "minimum": 0
            }
          },
          "additionalProperties": false,
          "required": [
            "DoesNotMeetPolicyMinimum"
          ]
        }
      ]
    }
  }
}