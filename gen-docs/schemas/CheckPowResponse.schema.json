{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "CheckPowResponse",
  "oneOf": [
    {
      "type": "string",
      "enum": [
        "Success"
      ]
    },
    {
      "type": "object",
      "properties": {
        "Failure": {
          "$ref": "#/$defs/PowFailureReason"
        }
      },
      "additionalProperties": false,
      "required": [
        "Failure"
      ]
    }
  ],
  "$defs": {
    "PowFailureReason": {
      "oneOf": [
        {
          "type": "string",
          "enum": [
            "FailedNoRetry",
            "NotFoundCanRetry",
            "BadRequestCanRetry",
            "BadIPCanRetry"
          ]
        },
        {
          "type": "object",
          "properties": {
            "DoesNotMeetPolicyMinimum": {
              "type": "integer",
              "format": "uint32",
              "minimum": 0
            }
          },
          "additionalProperties": false,
          "required": [
            "DoesNotMeetPolicyMinimum"
          ]
        }
      ]
    }
  }
}