{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "AuthenticateRequest",
  "description": "Requests an access token using a username and password",
  "type": "object",
  "properties": {
    "password": {
      "type": "string"
    },
    "username": {
      "type": "string"
    }
  },
  "required": [
    "username",
    "password"
  ]
}