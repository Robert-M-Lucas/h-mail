{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "PowFailureReason",
  "oneOf": [
    {
      "type": "string",
      "enum": [
        "FailedNoRetry",
        "NotFoundCanRetry",
        "BadRequestCanRetry",
        "BadIPCanRetry"
      ]
    },
    {
      "type": "object",
      "properties": {
        "DoesNotMeetPolicyMinimum": {
          "type": "integer",
          "format": "uint32",
          "minimum": 0
        }
      },
      "additionalProperties": false,
      "required": [
        "DoesNotMeetPolicyMinimum"
      ]
    }
  ]
}