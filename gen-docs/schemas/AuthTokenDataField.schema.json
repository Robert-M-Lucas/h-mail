{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "AuthTokenDataField",
  "description": "An `AuthToken` with attached expiry time",
  "type": "object",
  "properties": {
    "expires_at": {
      "$ref": "#/$defs/SystemTimeField"
    },
    "token": {
      "$ref": "#/$defs/AuthTokenField"
    }
  },
  "required": [
    "token",
    "expires_at"
  ],
  "$defs": {
    "AuthTokenField": {
      "description": "Represents a base-64 encoded authentication token.\nUsed in bearer tokens and in some requests.",
      "type": "string"
    },
    "SystemTimeField": {
      "description": "A timestamp represented as milliseconds since epoch",
      "type": "integer",
      "format": "uint128",
      "minimum": 0
    }
  }
}